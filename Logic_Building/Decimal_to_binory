import java.util.Scanner;
import java.lang.Math;
public class Main
{
    public static void main(String [] args)
    {
        int [] arr=new int[100];
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        System.out.println("");
        int n1,i=0;
        while(n!=0)
        {
            arr[i]=n%2;
            n=n/2;
            i++;
        }
        for(int j=i-1;j>=0;j--)
        {
        System.out.print(arr[j]);
        }
    }
}
