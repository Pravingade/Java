class Employee
{
    int id;
    String name,edu;
    Employee(int id,String name,String edu)
    {
        this.id=id;
        this.name=name;
        this.edu=edu;
    }
    public String toString()
    {
        return "Details: "+id+"  "+name+"  "+edu;
    }
}
public class Main
{
    public static void main(String []args)
    {
        System.out.println("main block...");
        Employee e=new Employee(101,"Pravin","Java");
        System.out.println(e);
    }
}
   
